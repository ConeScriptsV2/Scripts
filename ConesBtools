	local function MakeNewEmptyPart(s)
		local PartRet = nil 
		local lx = false
		local NewPartArgs = {
			[1] = "CreatePart",
			[2] = "Normal",
			[3] = CFrame.new(5039,9e9,9e9),
			[4] = game.Workspace
		}
		if s then NewPartArgs[2] = "Seat" end
		local function childaddedyieldfornewpart(ins)
			if not lx and ins:IsA("BasePart") then
				lx = true
				func:Disconnect()
				PartRet = ins
			end
		end
		func = game.Workspace.ChildAdded:Connect(childaddedyieldfornewpart)
		CallAPI(NewPartArgs)
		local CallWraps = false
		local FoundPart = false
		task.spawn(function()
			task.wait(4)
			if not FoundPart then
				warn("Discontinued Finding Part, Timed out.")
				CallWraps = true
			end
		end)
		while task.wait() do
			if CallWraps or FoundPart then
				break
			else
				if PartRet ~= nil then
					FoundPart = true
				end
			end
		end
		if CallWraps then
			return
		end
		return PartRet
	end
	local function SetSelected(tableofinstances)
		for i,v in pairs(script.Parent:GetChildren()) do
			if v:IsA("SelectionBox") then
				v:Destroy()
			end
		end
		SelectedParts = tableofinstances
		for i,v in pairs(SelectedParts) do
			local SelectBox = Instance.new("SelectionBox",script.Parent)
			SelectBox.Name = "SelectionBox"
			SelectBox.LineThickness = 0.06
			SelectBox.SurfaceTransparency = 0.7
			task.spawn(function()
				while task.wait() do
					SelectBox.Color3 = RGBColor
					SelectBox.SurfaceColor3 = RGBColor
				end
			end)
			SelectBox.Adornee = v
		end
	end
