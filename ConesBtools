local function FJZY_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local Tweninfo = TweenInfo.new(0.25,Enum.EasingStyle.Exponential,Enum.EasingDirection.In,0,false,0)
	local Tweninfo1 = TweenInfo.new(0.25,Enum.EasingStyle.Exponential,Enum.EasingDirection.Out,0,false,0)
	local Tweninfo2 = TweenInfo.new(0.4,Enum.EasingStyle.Exponential,Enum.EasingDirection.Out,0,false,0)
	local Tweninfo3 = TweenInfo.new(0.4,Enum.EasingStyle.Exponential,Enum.EasingDirection.In,0,false,0)
	local TweenService = game:GetService("TweenService")
	local AllowSwitch = true
	
	local function CallAPI(argsxsxs) --Will Yield
		if game.Players.LocalPlayer.Backpack:FindFirstChild("Building Tools") then
			game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Building Tools").SyncAPI.ServerEndpoint:InvokeServer(unpack(argsxsxs))
		else
			game:GetService("Players").LocalPlayer.Character:FindFirstChild("Building Tools").SyncAPI.ServerEndpoint:InvokeServer(unpack(argsxsxs))
		end
	end
	local CLP = {}
	local function NewChildLockedPart(bp : BasePart)
		table.insert(CLP,bp)
		local drd = false
		local Continue = true	
		task.spawn(function()
			while task.wait() do
				if table.find(CLP,bp) == nil then
					Continue = false
					break
				end
			end
		end)
		local function re(ins)
			if ins == bp and not drd then
				drd = true
				CallAPI({"CreateGroup","Model",bp.Parent,{bp}})
				CallAPI({"Ungroup",{bp.Parent}})
				wait()
				if bp == nil or bp.Parent == nil or bp.Parent.Parent == nil then
					drd = false
					re()
					warn("Failed to refit, Retrying")
				end
				drd = false
			end
		end
		game.Workspace.DescendantRemoving:Connect(re)
	end
	
	local function RemoveChildLockedPart(bp)
		local s,e = pcall(function()
			table.remove(CLP,table.find(CLP,bp))
		end)
		if s then else
			warn("Error with removing child locked part, "..e)
		end
	end
	
	local function SwapoutWithNewImageId(imageid : string,backwards : boolean)
		task.spawn(function()
			AllowSwitch = false
			if backwards then
				local clone = script.Parent.ImageLabel.ImageLabel:Clone()
				local og = script.Parent.ImageLabel.ImageLabel
				clone.ImageTransparency = 1
				clone.Image = imageid
				clone.Position = UDim2.new(0,0,0.5,0)
				clone.Parent = script.Parent.ImageLabel
				TweenService:Create(og,Tweninfo,{Position = UDim2.new(1,0,0.5,0)}):Play()
				TweenService:Create(og,Tweninfo, {ImageTransparency = 1}):Play()
				task.wait(0.17)
				local Sound = Instance.new("Sound",game:GetService("SoundService"))
				Sound.SoundId = "rbxassetid://6324790483"
				Sound.Volume = 0.5
				Sound.PlayOnRemove = true
				Sound:Destroy()
				TweenService:Create(clone,Tweninfo1,{Position = UDim2.new(0.5,0,0.5,0)}):Play()
				TweenService:Create(clone,Tweninfo1,{ImageTransparency = 0}):Play()
				task.wait(0.1)
				AllowSwitch = true
				og:Destroy()
			else
				local clone = script.Parent.ImageLabel.ImageLabel:Clone()
				local og = script.Parent.ImageLabel.ImageLabel
				clone.ImageTransparency = 1
				clone.Image = imageid
				clone.Position = UDim2.new(1,0,0.5,0)
				clone.Parent = script.Parent.ImageLabel
				TweenService:Create(og,Tweninfo,{Position = UDim2.new(0,0,0.5,0)}):Play()
				TweenService:Create(og,Tweninfo, {ImageTransparency = 1}):Play()
				task.wait(0.17)
				local Sound = Instance.new("Sound",game:GetService("SoundService"))
				Sound.SoundId = "rbxassetid://6324790483"
				Sound.Volume = 0.5
				Sound.PlayOnRemove = true
				Sound:Destroy()
				TweenService:Create(clone,Tweninfo1,{Position = UDim2.new(0.5,0,0.5,0)}):Play()
				TweenService:Create(clone,Tweninfo1,{ImageTransparency = 0}):Play()
				task.wait(0.1)
				AllowSwitch = true
				og:Destroy()
			end
		end)
	end
	
	local SelectedParts = {}
	local HighlightedParts = {}
	local RGBColor = Color3.fromHSV(0, 0.701961, 1)
	local AV = 0
	task.spawn(function()
		while task.wait() do
			RGBColor = Color3.fromHSV(AV / 255, 0.701961, 1)
