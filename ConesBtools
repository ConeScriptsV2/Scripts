				TooltipActive = false
			end
			if Sections[CurrentSection + 1] == nil then
				SwapoutWithNewImageId(Sections[1],false)
				CurrentSection = 1
				task.wait(0.2)
				script.Parent.TextLabel.Text = SectionNames[CurrentSection]
			else
				SwapoutWithNewImageId(Sections[CurrentSection + 1],false)
				CurrentSection = CurrentSection + 1
				task.wait(0.2)
				script.Parent.TextLabel.Text = SectionNames[CurrentSection]
			end
		end
	
		if inp.KeyCode == Enum.KeyCode.X and not gpe then
			if not TooltipActive then
				local Sound = Instance.new("Sound",game:GetService("SoundService"))
				Sound.SoundId = "rbxassetid://12221967"
				Sound.Volume = 0.6
				Sound.PlayOnRemove = true
				Sound:Destroy()
