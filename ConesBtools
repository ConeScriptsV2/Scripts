	local function AddNewButtonFunctionality(usingname : boolean,idorname,userightclick :boolean,functionn)
		task.spawn(function()
			if userightclick and usingname then
				local ID = nil
				for i,v in pairs(SectionNames) do
					if v == idorname then
						ID = i
					end
				end
				table.insert(RightClickSections,idorname.."|"..ID)
			elseif userightclick and not usingname then
				local Name = nil
				for i,v in pairs(SectionNames) do
					if i == idorname then
						Name = v
					end
				end
				table.insert(RightClickSections,Name.."|"..idorname)
			end
			if usingname then
				while task.wait() do
					while task.wait() do
						if TempCalledSectionName == idorname then
							TempCalledSectionName = ""
							TempCalledSectionId = 0
							break
						end
					end
					functionn()
				end
			else
				while task.wait() do
					while task.wait() do
						if TempCalledSectionId == idorname then
							TempCalledSectionName = ""
							TempCalledSectionId = 0
							break
						end
					end
					functionn()
				end
			end
		end)
	end
