			LateCameraCF = game.Workspace.CurrentCamera.CFrame
		end
	end)
	
	game:GetService("Players").LocalPlayer:GetMouse().Button1Up:Connect(function()
		if table.find(RightClickSections,SectionNames[CurrentSection].."|"..CurrentSection) then else
			TempCalledSectionId = CurrentSection
			TempCalledSectionName = SectionNames[CurrentSection]
		end
		--//Call Initialized Functions\\-- 
	end)
	game:GetService("Players").LocalPlayer:GetMouse().Button2Down:Connect(function()
		if table.find(RightClickSections,SectionNames[CurrentSection].."|"..CurrentSection) then
			TempCalledSectionId = CurrentSection
			TempCalledSectionName = SectionNames[CurrentSection]
		end
		--//Added this since i couldnt do it normally\\--
		if SectionNames[CurrentSection] == "Powerful Selection" then
			if game.Players.LocalPlayer:GetMouse().Target ~= nil then
				wait()
				if LateCameraCF == game.Workspace.CurrentCamera.CFrame then
				script.Parent.Parent.RCMPS.Position = UDim2.new(0,game.Players.LocalPlayer:GetMouse().X,0,game.Players.LocalPlayer:GetMouse().Y)
				SelectedPart = game.Players.LocalPlayer:GetMouse().Target
				if script.Parent.Parent.RCMPS.Visible == false then
					script.Parent.Parent.RCMPS.Visible = true
				else
					script.Parent.Parent.RCMPS.Visible = false
					end
					end
			end
		end
	end)
end
coroutine.wrap(FJZY_fake_script)()
local function QQLK_fake_script() -- Frame_3.LocalScript 
	local script = Instance.new('LocalScript', Frame_3)

	local Icons = {
		["Unfound Class"] = "http://www.roblox.com/asset/?id=86666555037412",
		["Part"] = "http://www.roblox.com/asset/?id=128736648980430",
		["BasePart"] = "http://www.roblox.com/asset/?id=128736648980430",
		["MeshPart"] = "http://www.roblox.com/asset/?id=89127677698494",
		["Model"] = "http://www.roblox.com/asset/?id=112460965108339",
		["Folder"] = "http://www.roblox.com/asset/?id=136851611126672",
		["Seat"] = "http://www.roblox.com/asset/?id=84638541845125",
		["Light"] = "http://www.roblox.com/asset/?id=94423120930012",
		["SpotLight"] = "http://www.roblox.com/asset/?id=94423120930012",
		["PointLight"] = "http://www.roblox.com/asset/?id=94423120930012",
		["UnionOperation"] = "http://www.roblox.com/asset/?id=136896600151322"
	}
	
	local CurrentPage = game.Workspace
	
	local function LoadPage(instance)
		-- Check if instance and its parent exist, otherwise return to Workspace
		if instance == nil or instance.Parent == nil then
			warn("Instance or its parent no longer exists. Returning to Workspace.")
			instance = game.Workspace
		end
	
		CurrentPage = instance
		script.Parent.GoBack.Visible = (CurrentPage ~= game.Workspace)
	
		-- Clear previous instance buttons
		for _, v in pairs(script.Parent:GetChildren()) do
			if v:IsA("TextButton") and v.Name == "Instance" then
				v:Destroy()
			end
		end
	
		wait()
	
		for _, v in pairs(instance:GetChildren()) do
			local icon = Icons[v.ClassName]--Icons["Unfound Class"] -- Default to "Unfound Class" icon if not found
			if icon then
				-- Create button for approved instances
				local ClonePart = script.Parent.InstanceExample:Clone()
				ClonePart.Name = "Instance"
				ClonePart.Parent = script.Parent
				ClonePart.Text = v.Name
				ClonePart.ImageLabel.Image = icon
	
				ClonePart.MouseButton1Click:Connect(function()
					LoadPage(v)
					CurrentPage = v
				end)
				ClonePart.Visible = true
			else
				warn("Unsupported Icon for Class: " .. v.ClassName)
			end
		end
	end
	
	script.Parent.GoBack.MouseButton1Click:Connect(function()
		if CurrentPage ~= game.Workspace then
			LoadPage(CurrentPage.Parent)
		end
	end) 
	
	local scrollingFrame = script.Parent -- Replace with the path to your ScrollingFrame
	local listLayout = scrollingFrame:FindFirstChildOfClass("UIListLayout")
	
	local function updateCanvasSize()
		-- Get total number of buttons and spacing
		local buttonCount = #scrollingFrame:GetChildren() - 1 -- excluding non-button elements like UIListLayout
		local buttonHeight = 25 -- Offset height of each button on Y-axis
		local spacing = listLayout.Padding.Offset -- Offset of spacing between buttons
	
		-- Calculate required canvas size and subtract the extra offset
		local totalHeight = (buttonCount * buttonHeight) + ((buttonCount - 1) * spacing) - 129
		scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, totalHeight)
	end
	
	-- Connect function to update when children change
	listLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(updateCanvasSize)
	scrollingFrame.ChildAdded:Connect(updateCanvasSize)
	scrollingFrame.ChildRemoved:Connect(updateCanvasSize)
	
	-- Initial call to set canvas size
	updateCanvasSize()
	
	LoadPage(game.Workspace)
end
coroutine.wrap(QQLK_fake_script)()
local function MNTSCRP_fake_script() -- Explorer.Smooth GUI Dragging 
	local script = Instance.new('LocalScript', Explorer)

	local UserInputService = game:GetService("UserInputService")
	local runService = (game:GetService("RunService"));
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	function Lerp(a, b, m)
		return a + (b - a) * m
	end;
	
	local lastMousePos
	local lastGoalPos
	local DRAG_SPEED = (10); -- // The speed of the UI darg.
	function Update(dt)
		if not (startPos) then return end;
		if not (dragging) and (lastGoalPos) then
			gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, lastGoalPos.X.Offset, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, lastGoalPos.Y.Offset, dt * DRAG_SPEED))
			return 
		end;
	
		local delta = (lastMousePos - UserInputService:GetMouseLocation())
		local xGoal = (startPos.X.Offset - delta.X);
		local yGoal = (startPos.Y.Offset - delta.Y);
		lastGoalPos = UDim2.new(startPos.X.Scale, xGoal, startPos.Y.Scale, yGoal)
		gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, xGoal, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, yGoal, dt * DRAG_SPEED))
	end;
	
	script.Parent.TopBar.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			lastMousePos = UserInputService:GetMouseLocation()
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	runService.Heartbeat:Connect(Update)
end
coroutine.wrap(MNTSCRP_fake_script)()
task.wait(3)
Opening.Visible = false
