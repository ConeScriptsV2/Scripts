				TweenService:Create(script.Parent.ToolTip,Tweninfo2,{Position = script.Parent.ToolTipgotopos.Position,TextTransparency = 0,BackgroundTransparency = 0.6}):Play()
				TooltipActive = true
			else
				local Sound = Instance.new("Sound",game:GetService("SoundService"))
				Sound.SoundId = "rbxassetid://12221967"
				Sound.Volume = 0.6
				Sound.PlayOnRemove = true
				Sound:Destroy()
	
				TweenService:Create(script.Parent.ToolTip,Tweninfo3,{Position = script.Parent.ToolTipStayPos.Position,TextTransparency = 1,BackgroundTransparency = 1}):Play()
				TooltipActive = false
			end
		end
	end)
	
	game:GetService("UserInputService").InputEnded:Connect(function(inp)
		if inp.KeyCode == Enum.KeyCode.LeftControl then
			ShiftDown = false
		end
	end)
	
