							[2] = p2,
							[3] = p3
						},
						[3] = Target.Character.Torso
					}
					CallAPI(PartLockArgs)
					--Unanchoring it so you can move--
					local MainArgs = {
						[1] = "SyncAnchor",
						[2] = {
							[1] = {
								["Anchored"] = false,
								["Part"] = p1
							},
							[2] = {
								["Anchored"] = false,
								["Part"] = p2
							},
							[3] = {
								["Anchored"] = false,
								["Part"] = p3
							}
						}
					}
					CallAPI(MainArgs)
					--Unlock Player To Move--
					TorsoAnchorArgs = {
						[1] = "SyncAnchor",
						[2] = {
							[1] = {
								["Anchored"] = false,
								["Part"] = Target.Character.Torso
							}
						}
					}
					CallAPI(TorsoAnchorArgs)
				end
			end)			
