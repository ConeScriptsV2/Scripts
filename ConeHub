						task.wait(2)
						if not FoundPart then
							warn("Discontinued Finding Part, Timed out.")
							CallWraps = true
						end
					end)
					while task.wait() do
						if CallWraps or FoundPart then
							break
						else
							if PartRet ~= nil then
								FoundPart = true
							end
						end
					end
					if CallWraps then
						return
					end
					return PartRet
